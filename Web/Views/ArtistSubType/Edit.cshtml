@model Web.Models.ArtistSubTypeModel

@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)
    <fieldset>
        <legend>ArtistSubTypeModel</legend>

        @Html.HiddenFor(model => model.Id)

        <div class="editor-label">
            @Html.LabelFor(model => model.ArtistSubType)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.ArtistSubType)
            @Html.ValidationMessageFor(model => model.ArtistSubType)
        </div>
        <div class="editor-label">
            @Html.LabelFor(model => model.ArtistType)
        </div>
        <div class="editor-field">
            @Html.DropDownListFor(model => model.ArtistType.Id, ViewBag.ArtistTypes as SelectList)
            @Html.ValidationMessageFor(model => model.ArtistType.Id)
        </div>

        <p>
            <input type="submit" value="Save" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>
