@model List<Web.Models.BagResult>

<h3>Browse Artists</h3>
<ul id="artists-category-list">
    @foreach (var item in Model)
    {
        <li>
            <a href="/Home/SearchType/@item.Id" data-typeId="@item.Id">@item.Name</a>
            <ul>
                @foreach (var subItem in item.Children)
                {
                    <li><a href="/Home/SearchCategory/@subItem.Id" data-subTypeId="@subItem.Id">@subItem.Name</a></li>
                }
            </ul>
        </li>    
    }
</ul>